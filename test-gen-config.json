{
    "enabled": "true",
    "increase-coverage": {
      "project-name": "eval-express-api-starter",
      "target-branches": [
        "main"
      ],
      "test-env-docker-stage": "test",
      "build-test-env-docker-stage-script-path": "tool/build-test-env-docker-stage.sh",
      "test-files-folders": [
        "test",
        "coverage"
      ],
      "source-test-file-paths": [
        {
          "source-file-path": "src/app.js",
          "test-file-path": "test/app.test.js"
        }
      ],
      "test-command": "npm run coverage",
      "coverage-type": "lcov",
      "code-coverage-report-path": "coverage/lcov.info",
      "desired-coverage": 100,
      "max-iterations": 2,
      "max-number-of-test-cases": 6
      }
  }